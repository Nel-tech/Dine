{
  "version": 2,
  "name": "dine-restaurant-site",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],

  "regions": ["sfo1", "iad1"],
  "alias": "dine-restaurant-site.vercel.app"
}
